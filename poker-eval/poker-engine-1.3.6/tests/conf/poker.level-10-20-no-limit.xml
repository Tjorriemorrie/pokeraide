<?xml version="1.0" encoding="ISO-8859-1"?>
<bet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="bet.xsd" name="no limit" table-stakes="yes" unit="1000" buy-in="200000" poker_engine_version="1.0.11">

    <description>No limit tournament</description>

    <blind change="levels" frequency="8" unit="minutes" levels="poker.levels-blinds.xml" />

    <all_in method="side"/> 

    <variants ids="omaha omaha8 holdem">
      <round name="pre-flop" min="big" />
      <round name="flop" min="big" />
      <round name="turn" min="big" />
      <round name="river" min="big" />
    </variants>

    <payouts>poker.payouts.xml</payouts>

</bet>

